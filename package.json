{
  "name": "learn-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "webpack": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js  --env.mode development",
    "debugthis": "node --inspect --inspect-brk ./webpack.config.js",
    "dev": "webpack-dev-server  --env.mode=development --hot",
    "dev:bug": "npm run debug -- --env.mode=development",
    "dev:debugthis": "npm run debugthis -- --env.mode=development ",
    "build": "npm run build:dev && npm run  build:prod",
    "build:dev": "npm run  webpack -- --env.mode development",
    "build:prod": "npm run  webpack -- --env.mode production",
    "prod:analyze": "npm run build:prod -- --env.presets analyze",
    "prod:compress": "npm run build:prod -- --env.presets compress",
    "prod:babel": "npm run build:prod -- --env.presets babel",
    "dev:babel": "npm run dev -- --env.presets babel"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "^3.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.3",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {}
}
